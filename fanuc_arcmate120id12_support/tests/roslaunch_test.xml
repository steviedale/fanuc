<?xml version="1.0"?>
<launch>
  <arg name="ip_str" value="127.0.0.1" />

  <group ns="load_arcmate120id12__">
    <include file="$(find fanuc_arcmate120id12_support)/launch/load_arcmate120id12.launch"/>
  </group>

  <group ns="test_arcmate120id12__">
    <include file="$(find fanuc_arcmate120id12_support)/launch/test_arcmate120id12.launch"/>
  </group>

  <group ns="robot_interface_streaming_arcmate120id12__">
    <include file="$(find fanuc_arcmate120id12_support)/launch/robot_interface_streaming_arcmate120id12.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>

  <group ns="robot_state_visualize_arcmate120id12__">
    <include file="$(find fanuc_arcmate120id12_support)/launch/robot_state_visualize_arcmate120id12.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>


  <!-- without bswap -->
  <group ns="robot_interface_streaming_arcmate120id12_f__">
    <include file="$(find fanuc_arcmate120id12_support)/launch/robot_interface_streaming_arcmate120id12.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
      <arg name="use_bswap" value="false" />
    </include>
  </group>

  <group ns="robot_state_visualize_arcmate120id12_f__">
    <include file="$(find fanuc_arcmate120id12_support)/launch/robot_state_visualize_arcmate120id12.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
      <arg name="use_bswap" value="false" />
    </include>
  </group>

</launch>
